<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chetan.kitnapyara">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/chetan_yellow"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.chetan.credits">
            <intent-filter>
                <action android:name="com.example.chetan.credits" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.chetan.Receive" android:configChanges="orientation|screenSize">
            <intent-filter>
                <action android:name="com.example.chetan.Receive" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


        <activity android:name="com.example.chetan.Result">
            <intent-filter>
                <action android:name="com.example.chetan.Result"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>
    </application>

</manifest>